{
  "name": "tourze/hotel-profile-bundle",
  "description": "酒店档案模块",
  "type": "symfony-bundle",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "doctrine/collections": "^2.3",
    "doctrine/dbal": "^4.0",
    "doctrine/doctrine-bundle": "^2.13",
    "doctrine/orm": "^3.0",
    "doctrine/persistence": "^3.1 || ^4",
    "easycorp/easyadmin-bundle": "^4",
    "psr/log": "^3|^2|^1",
    "symfony/config": "^6.4",
    "symfony/console": "^6.4",
    "symfony/dependency-injection": "^6.4",
    "symfony/doctrine-bridge": "^6.4",
