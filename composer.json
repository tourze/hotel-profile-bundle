{
  "name": "tourze/hotel-profile-bundle",
  "description": "酒店档案模块",
  "type": "symfony-bundle",
  "license": "MIT",
  "require": {
    "php": "^8.1",
    "symfony/config": "^6.4",
    "symfony/console": "^6.4",
    "symfony/dependency-injection": "^6.4",
    "symfony/framework-bundle": "^6.4",
    "symfony/http-kernel": "^6.4",
    "symfony/yaml": "^6.4 || ^7.1"
  },
  "require-dev": {
    "phpstan/phpstan": "^2.1",
    "phpunit/phpunit": "^10.0"
  },
  "autoload": {
    "psr-4": {
      "Tourze\\HotelProfileBundle\\": "src"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "Tourze\\HotelProfileBundle\\Tests\\": "tests"
    }
  }
}